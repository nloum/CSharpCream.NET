            for (int st1 = d1.min(); st1 <= d1.max(); st1 += d1.Step)
            {
                bool found = false;
                int lt1 = st1 + d1.Duration;
                for (int st2 = d2.min(); st2 <= d2.max(); st2 += d2.Step)
                {
                    int lt2 = st2 + d2.Duration;
                    if ((st1 == st2) && (lt1 == lt2))
